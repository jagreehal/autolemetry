{
  "name": "autolemetry-adapters",
  "version": "1.0.2",
  "description": "Write Once, Observe Anywhere - Analytics adapters for autolemetry (PostHog, Mixpanel, Amplitude, Segment)",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./posthog": {
      "types": "./dist/posthog.d.ts",
      "import": "./dist/posthog.js"
    },
    "./mixpanel": {
      "types": "./dist/mixpanel.d.ts",
      "import": "./dist/mixpanel.js"
    },
    "./segment": {
      "types": "./dist/segment.d.ts",
      "import": "./dist/segment.js"
    },
    "./amplitude": {
      "types": "./dist/amplitude.d.ts",
      "import": "./dist/amplitude.js"
    },
    "./webhook": {
      "types": "./dist/webhook.d.ts",
      "import": "./dist/webhook.js"
    },
    "./factories": {
      "types": "./dist/factories.d.ts",
      "import": "./dist/factories.js"
    },
    "./middleware": {
      "types": "./dist/middleware.d.ts",
      "import": "./dist/middleware.js"
    },
    "./testing": {
      "types": "./dist/testing/index.d.ts",
      "import": "./dist/testing/index.js"
    }
  },
  "files": [
    "dist",
    "src",
    "examples",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "npx eslint 'src/**/*.ts' 'examples/**/*.ts'",
    "lint:fix": "npx eslint 'src/**/*.ts' 'examples/**/*.ts' --fix",
    "type-check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "keywords": [
    "autolemetry",
    "opentelemetry",
    "analytics",
    "posthog",
    "mixpanel",
    "amplitude",
    "segment",
    "webhook",
    "adapters",
    "business-metrics",
    "product-analytics",
    "telemetry",
    "observability"
  ],
  "author": "Jag Reehal<jag@jagreehal.com> (https://jagreehal.com)",
  "license": "MIT",
  "peerDependencies": {
    "autolemetry": "workspace:*"
  },
  "peerDependenciesMeta": {
    "posthog-node": {
      "optional": true
    },
    "mixpanel": {
      "optional": true
    },
    "@segment/analytics-node": {
      "optional": true
    },
    "@amplitude/analytics-node": {
      "optional": true
    }
  },
  "devDependencies": {
    "@amplitude/analytics-node": "^1.5.22",
    "@arethetypeswrong/cli": "^0.18.2",
    "@cloudflare/workers-types": "^4.20251115.0",
    "@prisma/client": "^6.19.0",
    "@segment/analytics-node": "^2.3.0",
    "@types/node": "^24.10.1",
    "@typescript-eslint/eslint-plugin": "^8.46.4",
    "@typescript-eslint/parser": "^8.46.4",
    "autolemetry": "workspace:*",
    "drizzle-orm": "^0.44.7",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-unicorn": "^62.0.0",
    "mixpanel": "^0.19.1",
    "mongoose": "^8.19.4",
    "posthog-node": "^5.11.2",
    "prettier": "^3.6.2",
    "rimraf": "^6.1.0",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.4",
    "vitest": "^4.0.9"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jagreehal/autolemetry"
  },
  "bugs": {
    "url": "https://github.com/jagreehal/autolemetry/issues"
  },
  "homepage": "https://github.com/jagreehal/autolemetry#readme"
}
